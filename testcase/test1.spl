/*
	1. 常量定义
	2. 赋值语句
*/ 
program hello;
const
	a = 1;
	b = 1.01;
	c = "abcd";
	d = 'c';
	e = true;
	f = false;

var
    A : integer;
    B : real;
    C : string;
    D : char;
    E : boolean;


begin
	A := a;
	B := b;
	C := c;
	D := d;
	E := f;
	printf("%d\n",A);
	printf("%f\n",B);
	printf("%s\n",C);
	printf("%c\n",D);
	printf("%d\n",E);
end
.