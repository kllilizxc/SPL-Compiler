/*
	1. 常量定义
	2. 赋值语句
*/ 
program hello;
const
	a = 1;
	b = 1.01;
	c = "abcd";
	d = 'c';
	e = true;
	f = false;
	g = maxint;

var
    I : integer;
    D : real;
    C : char;
    S : string;


begin
	D := b;
	I := e;
	S := c;
	C := d;
	printf("%f\n",D);
	printf("%d\n",e);
	printf("%s\n",S);
	printf("%c\n",C);
end
.