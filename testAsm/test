; ModuleID = 'Mine'
source_filename = "Mine"

@i = internal global i32 zeroinitializer, align 4

declare i32 @printf(i8*, ...)

define void @hello() {
hello:
  %i = load i32, i32* @i
  switch i32 %i, label %exit [
    i32 1, label %case
    i32 2, label %case1
  ]

case:                                             ; preds = %hello
  store i32 1, i32* @i

case1:                                            ; preds = %hello
  store i32 2, i32* @i

exit:                                             ; preds = %hello
  %0 = alloca [3 x i8]
  store [3 x i8] c"%d\00", [3 x i8]* %0
  %1 = bitcast [3 x i8]* %0 to i8*
  %i2 = load i32, i32* @i
  %2 = call i32 (i8*, ...) @printf(i8* %1, i32 %i2)
  ret void
}
